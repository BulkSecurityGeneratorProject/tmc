    <queryString>
        <![CDATA[SELECT
	COUNT(T.STATUS_TOPICO) AS QUANTIDADE,
    STATUS_TOPICO
FROM
	TOPICO T
WHERE
	CAST(T.DATA AS DATE)  >= CAST( $P{DATA_INICIO}  AS DATE)
	AND CAST(T.DATA AS DATE)  <= CAST( $P{DATA_FIM}  AS DATE)
GROUP BY T.STATUS_TOPICO]]>
